<?xml version='1.0' encoding='utf-8'?>
<widget id="com.geo.geofencing" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <name>GeoFencing</name>
    <description>
        App for geofencing
    </description>
    <author email="dafale.nikhil@gmail.com" href="">
        #n!k.h
    </author>
    <content src="index.html" />
    <access origin="*" />
    <config-file mode="merge" parent="/manifest" platform="android">
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
        <uses-feature android:name="android.hardware.location.gps" />
    </config-file>
    <preference name="loglevel" value="DEBUG" />
</widget>
